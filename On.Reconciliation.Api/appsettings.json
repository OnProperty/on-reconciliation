{
  "Serilog": {
    "LevelSwitches": {
      "$controlSwitch": "Debug"
    },
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default" :  "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "Application": "On.Reconciliation.Api",
      "Tenant": ""
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "levelSwitch": "$controlSwitch"
        }
      },
      {
        "Name": "File",
        "Args": {
          "levelSwitch": "$controlSwitch",
          "rollingInterval": "Day",
          "path": "D:\\Temp\\Logs\\log.txt"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "apiKey": null,
          "controlLevelSwitch": "$controlSwitch"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
